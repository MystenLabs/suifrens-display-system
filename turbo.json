{
	"$schema": "https://turbo.build/schema.json",
	"pipeline": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**"]
		},
		"lint": {},
		"//#lint": {},
		"dev": {
			"dependsOn": ["^build"],
			"cache": false,
			"persistent": true
		}
	},
	"globalEnv": [
		"VITE_VERCEL_ENV",
		"VITE_ACCESSORIES_PACKAGE_ID",
		"VITE_ACCESSORIES_STORE",
		"VITE_API_ENV",
		"VITE_BACKEND_API",
		"VITE_CAPY_LABS",
		"VITE_MINT_APP",
		"VITE_PACKAGE_ID",
		"VITE_PACKAGE_ID_V2",
		"VITE_SENTRY_SAMPLE_RATE",
		"VITE_SUI_NETWORK_NAME",
		"VITE_SUI_NETWORK"
	]
}
