{
	"name": "@suifrens/shared",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"source": "./src/index.ts",
			"import": "./dist/index.mjs",
			"require": "./dist/index.js"
		}
	},
	"private": true,
	"sideEffects": false,
	"version": "0.0.0",
	"scripts": {
		"build": "pnpm build:types && tsup ./src/index.ts --format esm,cjs --sourcemap",
		"build:types": "tsc --build"
	},
	"dependencies": {
		"@growthbook/growthbook": "^0.25.0",
		"@mysten/sui.js": "^0.46.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@types/node": "^20.3.3",
		"@types/react": "^18.0.27",
		"@types/react-dom": "^18.0.9",
		"tsup": "^6.7.0",
		"typescript": "^5.0.4"
	}
}
